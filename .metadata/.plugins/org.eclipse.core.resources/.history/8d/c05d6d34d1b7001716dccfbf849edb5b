package com.sg.service;

public class Mtable {
private int num;

public Mtable(int num) throws Exception {
	super();
	if(num  <= 0)
		throw new Exception("Number cannot be Zero");
	this.num = num;
}
public void display(){
	for(int i=1; i<=10; ++i)
		System.out.println("%d * %d = %d\n",i,num,(num*i));
		
}
}
