package com.sg.service;

public class Emp {
private String name;
private int age;

public String getName() {
	return name;
}
public void setName(String name) {
	this.name = name;
}
public int getAge() {
	return age;
}
public void setAge(int age) {
	if(age < 20 && age>60){
		throw new Exception("Age must be between 20 and 60 ")
	}
	this.age = age;
}
}
